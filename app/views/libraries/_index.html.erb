<table>
    <thead>
        <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Cover</th>
        <th colspan="3"></th>
        <th>Ajax</th>
        </tr>
    </thead>

    <tbody>
        <% @libraries.each do |library| %>
            <tr>
                <td><%= library.title %></td>
                <td><%= library.description %></td>
                <td><%= library.cover %></td>
                <td><%= link_to 'Show', library %></td>
                <td><%= link_to 'Edit', edit_library_path(library) %></td>
                <td><%= link_to 'Destroy', library, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                <td>
                    <button onclick="viewlib(<%= library.id %>)">Ajax</button>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>