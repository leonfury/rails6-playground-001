<p id="notice"><%= notice %></p>

<h1>Libraries</h1>

<table>
    <thead>
        <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Cover</th>
        <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @libraries.each do |library| %>
            <tr>
                <td><%= library.title %></td>
                <td><%= library.description %></td>
                <td><%= library.cover %></td>
                <td><%= link_to 'Show', library %></td>
                <td><%= link_to 'Edit', edit_library_path(library) %></td>
                <td><%= link_to 'Destroy', library, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<br>

<%= link_to 'New Library', new_library_path %>
<button onclick="listenerButton()">Onclick JS function from pack/home.js</button>

<button id="custombutton">Event Listener on App JS</button>

<button onclick="listenerCuba()">Onclick JS function from custom/cuba.js</button>